<div id="main">
<h2>Bewerk locatie</h2>
<p>Bewerk locatie van <%= @work.stock_number%> <%= @work.name%></p>
<%= simple_form_for([@collection,@work]) do |f| %>
<%= f.input :location %>
<%= f.input :location_floor %>
<%= f.input :location_detail %>
<%= f.association :work_status %>

<%= f.submit class: :button%>
<% end%>
</div>