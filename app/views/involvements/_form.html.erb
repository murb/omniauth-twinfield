<%= simple_form_for(@involvement) do |f| %>
  <%= f.input :name %>
  <%= f.input :place %>
  <label>Plaatsnamen database koppeling</label>
  <%= f.select :place_geoname_id, { @involvement.place_geoname_name => @involvement.place_geoname_id}, {}, class: 'select2 ajax' %>
  <hr/>
  <%= f.submit class: :button %>

<% end %>
