<%= simple_form_for([@collection, @import_collection]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :collection_id %>
    <%= f.input :file %>
    <%= f.input :settings %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: :button %>
  </div>
<% end %>
