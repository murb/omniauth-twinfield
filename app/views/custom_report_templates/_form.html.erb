<%= simple_form_for(@custom_report_template) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :text %>
    <%= f.association :collection %>
    <%= f.input :work_fields %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
